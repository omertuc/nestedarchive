[distutils]
index-servers =
    pypi
    pypitest

[pypi]
username = __token__
password = <token from https://pypi.org/manage/account/token/>
repository = https://upload.pypi.org/legacy/

[pypitest]
username = __token__
password = <token from https://test.pypi.org/manage/account/token/>
repository = https://test.pypi.org/legacy/
